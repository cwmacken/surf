<div class="row">
	<div class="col-md-offset-1 pageHeader">
		PLEASE SIGN UP
	</div>
</div>
<div class="row"> 
	<div class="col-md-offset-1 tableText">		
		
			<%= form_for @user do |user| %>
		<p>
			<%= user.label :name, "Name", class:'inputTextAlt'%>
			<%= user.text_field :name, class:'inputReset'%>
		</p>
		<p>
			<%= user.label :height, "Height", class:'inputTextAlt'%>
			<%= user.text_field :height, class:'inputReset'%>
		</p>
		<p>
			<%= user.label :weight, "Weight", class:'inputTextAlt'%>
			<%= user.text_field :weight, class:'inputReset'%>
		</p>
		<p>
			<%= user.label :skillLevel, "Skill Level", class:'inputTextAlt'%>
			<%= user.text_field :skillLevel, class:'inputReset'%>
		</p>
		<p>
			<%= user.label :email, "Email", class:'inputTextAlt'%>
			<%= user.text_field :email, class:'inputReset'%>
		</p>

		<p>
			<%= user.label :password, "Password", class:'inputTextAlt'%>
			<%= user.password_field :password, class:'inputReset'%>
		</p>

		<p>
			<%= user.label :password_confirmation, "Confirm Password", class:'inputTextAlt'%>
			<%= user.password_field :password_confirmation, class:'inputReset'%>
		</p>
		<p>
			<%= user.label :shaper, "Shaper?", class:'inputTextAlt'%>
			<%= user.check_box :shaper, {}, "true", "false" %>
		</p>

		<p>
			<%= user.submit "Sign Up", class:"btnmain" %>
		</p>
		<button class="btnmain"><a class="btnkLink" id="signUp"href='/authentications/new'>Back to Sign In</a></button>


		<%end%>
	</div>	
<div class="row">